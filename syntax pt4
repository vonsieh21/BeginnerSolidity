//import solidity
pragma solidity ^0.4.0;

contract Simple {
    function calcs (uint _a, uint _b) public pure    //uses uint _a and uint _b to use for the function
    returns (uint o_sum, uint o_product) {    //returns function returns the values in the parameters
        o_sum = _a + _b;
        o_product = _a * _b;
    }
}
